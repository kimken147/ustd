#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.28.6_@babel+core@7.28.6__@babel+plugin-_f78f3e1bb16fad5c70132fd65c06a45d/node_modules/react-scripts/bin/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.28.6_@babel+core@7.28.6__@babel+plugin-_f78f3e1bb16fad5c70132fd65c06a45d/node_modules/react-scripts/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.28.6_@babel+core@7.28.6__@babel+plugin-_f78f3e1bb16fad5c70132fd65c06a45d/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.28.6_@babel+core@7.28.6__@babel+plugin-_f78f3e1bb16fad5c70132fd65c06a45d/node_modules/react-scripts/bin/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.28.6_@babel+core@7.28.6__@babel+plugin-_f78f3e1bb16fad5c70132fd65c06a45d/node_modules/react-scripts/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@babel+plugin-syntax-flow@7.28.6_@babel+core@7.28.6__@babel+plugin-_f78f3e1bb16fad5c70132fd65c06a45d/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../../bin/react-scripts.js" "$@"
fi
