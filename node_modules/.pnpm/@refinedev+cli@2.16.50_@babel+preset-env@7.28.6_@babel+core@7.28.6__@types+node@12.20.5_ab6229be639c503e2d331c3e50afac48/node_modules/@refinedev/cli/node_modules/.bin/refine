else
  export NODE_PATH="/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+cli@2.16.50_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+node@12.20.5_ab6229be639c503e2d331c3e50afac48/node_modules/@refinedev/cli/dist/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+cli@2.16.50_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+node@12.20.5_ab6229be639c503e2d331c3e50afac48/node_modules/@refinedev/cli/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+cli@2.16.50_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+node@12.20.5_ab6229be639c503e2d331c3e50afac48/node_modules/@refinedev/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+cli@2.16.50_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+node@12.20.5_ab6229be639c503e2d331c3e50afac48/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/node_modules:$NODE_PATH"
