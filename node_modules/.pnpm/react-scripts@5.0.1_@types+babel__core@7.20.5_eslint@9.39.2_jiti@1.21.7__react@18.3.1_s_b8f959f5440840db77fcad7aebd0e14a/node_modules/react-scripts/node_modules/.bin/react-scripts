#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@types+babel__core@7.20.5_eslint@9.39.2_jiti@1.21.7__react@18.3.1_s_b8f959f5440840db77fcad7aebd0e14a/node_modules/react-scripts/bin/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@types+babel__core@7.20.5_eslint@9.39.2_jiti@1.21.7__react@18.3.1_s_b8f959f5440840db77fcad7aebd0e14a/node_modules/react-scripts/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@types+babel__core@7.20.5_eslint@9.39.2_jiti@1.21.7__react@18.3.1_s_b8f959f5440840db77fcad7aebd0e14a/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@types+babel__core@7.20.5_eslint@9.39.2_jiti@1.21.7__react@18.3.1_s_b8f959f5440840db77fcad7aebd0e14a/node_modules/react-scripts/bin/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@types+babel__core@7.20.5_eslint@9.39.2_jiti@1.21.7__react@18.3.1_s_b8f959f5440840db77fcad7aebd0e14a/node_modules/react-scripts/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/react-scripts@5.0.1_@types+babel__core@7.20.5_eslint@9.39.2_jiti@1.21.7__react@18.3.1_s_b8f959f5440840db77fcad7aebd0e14a/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/react-scripts.js" "$@"
else
  exec node  "$basedir/../../bin/react-scripts.js" "$@"
fi
