#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+devtools-server@2.0.1_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+re_bdecd2c9d948eb5386ec57b585cfa547/node_modules/@refinedev/devtools-server/dist/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+devtools-server@2.0.1_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+re_bdecd2c9d948eb5386ec57b585cfa547/node_modules/@refinedev/devtools-server/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+devtools-server@2.0.1_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+re_bdecd2c9d948eb5386ec57b585cfa547/node_modules/@refinedev/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+devtools-server@2.0.1_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+re_bdecd2c9d948eb5386ec57b585cfa547/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+devtools-server@2.0.1_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+re_bdecd2c9d948eb5386ec57b585cfa547/node_modules/@refinedev/devtools-server/dist/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+devtools-server@2.0.1_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+re_bdecd2c9d948eb5386ec57b585cfa547/node_modules/@refinedev/devtools-server/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+devtools-server@2.0.1_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+re_bdecd2c9d948eb5386ec57b585cfa547/node_modules/@refinedev/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/@refinedev+devtools-server@2.0.1_@babel+preset-env@7.28.6_@babel+core@7.28.6__@types+re_bdecd2c9d948eb5386ec57b585cfa547/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@refinedev/devtools-server/dist/cli.cjs" "$@"
else
  exec node  "$basedir/../@refinedev/devtools-server/dist/cli.cjs" "$@"
