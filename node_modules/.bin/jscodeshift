#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/jscodeshift@17.3.0_@babel+preset-env@7.28.6_@babel+core@7.28.6_/node_modules/jscodeshift/bin/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/jscodeshift@17.3.0_@babel+preset-env@7.28.6_@babel+core@7.28.6_/node_modules/jscodeshift/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/jscodeshift@17.3.0_@babel+preset-env@7.28.6_@babel+core@7.28.6_/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/jscodeshift@17.3.0_@babel+preset-env@7.28.6_@babel+core@7.28.6_/node_modules/jscodeshift/bin/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/jscodeshift@17.3.0_@babel+preset-env@7.28.6_@babel+core@7.28.6_/node_modules/jscodeshift/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/jscodeshift@17.3.0_@babel+preset-env@7.28.6_@babel+core@7.28.6_/node_modules:/Users/apple/projects/morgan/ustd/.worktrees/monorepo-refine-upgrade/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../jscodeshift/bin/jscodeshift.js" "$@"
else
  exec node  "$basedir/../jscodeshift/bin/jscodeshift.js" "$@"
fi
